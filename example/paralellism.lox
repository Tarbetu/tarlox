fun factorial(x) {
	fun factorial(acc, n) {
		if (n < 2) {
			return acc;
		}
		return factorial(acc * n, n - 1);
	}

	return factorial(1, x);
}

await_var begin = clock_millis();
var x = factorial(42);
var y = factorial(420);
await_var awaited_x = x;
await_var awaited_y = y;
await_var end = clock_millis();
print("factorial(42) = " + x);
print("factorial(420) = " + y);
print "It tooks " + (end - begin) + " milliseconds!";
